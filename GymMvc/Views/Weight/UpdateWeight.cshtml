@model GymMvc.Models.UpdateWeight
<h5>
    <i class="bi bi-building-fill-add"></i>
    Update A Weight
</h5>
<form asp-controller="Weight" enctype="multipart/form-data">
    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                <label asp-for="NewWeight" class="form-label mt-4"></label>
                <input type="text" class="form-control" asp-for="NewWeight" placeholder="Name">
                <span asp-validation-for="NewWeight" class="text-danger"></span>
            </div>
            
                <button type="submit" class="btn btn-light text-black mt-4" onClick="history.go(-1); return true;">Save</button>
            <a href="##" onClick="history.go(-1); return false;"> Return to Your Page</a>

            
        </div>
    </div>
</form>
<div>
</div>
